{
  "name": "mysqlserver",
  "default_attributes": { },
  "override_attributes": {
    "mysql": {
      "server_root_password": "password",
      "server_repl_password": "password",
      "server_debian_password": "password",
      "tunable": {
        "log_slow_queries": "/var/log/mysql/mysqld-slow.log",
        "long_query_time": "0"
      }
    },
    "yum": {
      "remi": {
        "includepkgs": "compat-mysql* mysql*"
      }
    }
  },
  "json_class": "Chef::Role",
  "description": "",
  "chef_type": "role",
  "run_list": [
    "iptables-stop",
    "yum",
    "yum::remi",
    "mysql::server",
    "mysql::client",
    "redhat-lsb",
    "td-agent::mysql"
  ]
}
